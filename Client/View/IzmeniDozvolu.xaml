<Window x:Class="Client.View.IzmeniDozvolu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.View"
        mc:Ignorable="d"
        Title="IzmeniDozvolu" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush  Viewport="0,0,1,1"   ImageSource="/Client;component/autoskola.jpg" />
        </Grid.Background>
        <Button Content="Promeni" Command="{Binding IzmeniDozvolu}" HorizontalAlignment="Left" Height="29" Margin="316,310,0,0" VerticalAlignment="Top" Width="93"/>
        <Label Content="Ime" Foreground="White" HorizontalAlignment="Left" Height="28" Margin="183,191,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="1.999,0.799"/>
        <TextBox HorizontalAlignment="Left" Height="28" Margin="273,191,0,0" Text="{Binding ChangeRa.nazivDozvole, UpdateSourceTrigger=PropertyChanged}" TextWrapping="Wrap"  VerticalAlignment="Top" Width="174"/>
    </Grid>
</Window>
